<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">

    <changeSet  author="liquibase-1"  id="avinash">
        <tagDatabase  tag="${REQUEST_NUMBER}-${BUILD_NUMBER}"/>
    </changeSet>
    <changeSet id="liquibase-2" author="avinash">
        <sqlFile path="/dbscripts/users.sql"/>
    <rollback>
        <sqlFile path="rollback/users_rollback.sql"/>
    </rollback>
    </changeSet>
    <changeSet id="liquibase-3" author="avinash">
        <sqlFile path="dbscripts/order.sql"/>
    <rollback>
        <sqlFile path="rollback/order_rollback.sql"/>
    </rollback>
    </changeSet>
        <changeSet id="liquibase-4" author="avinash">
        <sqlFile path="dbscripts/user_update.sql"/>
    <rollback>
        <sqlFile path="rollback/user_update_rollback.sql"/>
    </rollback>
    </changeSet>
</databaseChangeLog>
